# Example .env file for Featurizer Service
# Copy this file to .env and modify as needed

# DeID Service URL
DEID_BASE_URL=http://localhost:8000

# Database configuration
DATABASE_URL=postgresql://postgres:root@localhost:5432/featDB

# NLP Configuration
USE_BIOBERT=false

# Optional: Individual database components (alternative to DATABASE_URL)
# DB_HOST=localhost
# DB_PORT=5432
# DB_USER=postgres
# DB_PASSWORD=root
# DB_NAME=featDB

# Optional: Service configuration
# LOG_LEVEL=INFO
# API_HOST=0.0.0.0
# API_PORT=8001